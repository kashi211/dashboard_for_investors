extends ../layout

block content
	.columns(style="margin-left: 1%;  width: 98%; position: flex")
		.column.is-2.is-sidebar-menu.is-hidden-mobile
			aside.menu
				p.menu-label
					| General
				ul.menu-list
					li
						a(href="/dashboard").is-active Dashboard
